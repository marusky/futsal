<div>
  <h1 class="font-bold text-4xl">Timy</h1>
  <ul>
    <% @game.teams.each do |team| %>
      <li>
        <%= team.name %> -
        <% team.players.each do |player| %>
          <%= player.name %>
        <% end %>
      </li>
    <% end %>
  </ul>
  <%= link_to 'Plus', new_team_path(game_id: @game.id) %>

  <%= button_to 'Zacat hru', start_game_path(@game), method: :patch %>
</div>
